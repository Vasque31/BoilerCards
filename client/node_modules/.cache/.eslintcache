ent.preventDefault();\n        const newAccountInfo = {\n            oldusername:oldusername,\n            newusername:newusername,\n            oldpassword:oldpassword,\n            newpassword:newpassword\n        }\n        console.log(newAccountInfo);\n        if(checkvalidpassword(newpassword)&&checkvalidusername(newusername)){\n            let res = await axios.post(\"http://localhost:3001/changecredential\", {\n            oldusername:oldusername,\n            newusername:newusername,\n            oldpassword:oldpassword,\n            newpassword:newpassword\n            \n        });\n        if(res.data ===true){\n            alert(\"success\");\n            window.location.reload();\n        }else{\n            alert(\"password does not match\")\n        }\n        console.log(newAccountInfo);\n        }else{\n            alert(\"wrong format\");\n        }\n        \n    }\n\n    return (\n        <div className=\"general-box-create\">\n            <div style={{textAlign: \"left\", fontSize: \" 1.5rem\"}}>\n                    <CloseButton variant= \"white\" onClick={() => navigate(-1)}/>\n            </div>\n            <Form>\n                <Form.Group style={{color: \"gold\"}}>\n                    <Form.Label>Old Username</Form.Label>\n                    <Form.Control type=\"text\" name= \"oldusername\" placeholder=\"oldusername\" onChange={e => setOldUsername(e.target.value)}/>\n                </Form.Group>\n\n                <Form.Group style={{color: \"gold\"}}>\n                    <Form.Label>Old Password</Form.Label>\n                    <Form.Control type=\"text\" name= \"oldpassword\" placeholder=\"oldpassword\" onChange={e => setOldPassword(e.target.value)} />\n                </Form.Group>\n                <Form.Group style={{color: \"gold\"}}>\n                    <Form.Label>New Username</Form.Label>\n                    <Form.Control type=\"text\" name= \"newusername\" placeholder=\"newusername\" onChange={e => setNewUsername(e.target.value)}/>\n                </Form.Group>\n\n                <Form.Group style={{color: \"gold\"}}>\n                    <Form.Label>New Password</Form.Label>\n                    <Form.Control type=\"text\" name= \"newpassword\" placeholder=\"newpassword\" onChange={e => setNewPassword(e.target.value)} />\n                </Form.Group>\n            </Form>\n\n            <div style={{paddingTop: \"1rem\"}}>\n                <Button variant=\"primary\" type=\"submit\" onClick={handleSave}>\n                    Update Credentials\n                </Button>\n            </div>\n        </div>\n    );\n}\n\nexport default ChangeCredentials;\n","/Users/jojo/Desktop/CS307/BoilerCards/client/src/Components/EditFlashcard.js",[],[],"/Users/jojo/Desktop/CS307/BoilerCards/client/src/Components/HomeLibrary.js",["164","165","166","167"],[],"/Users/jojo/Desktop/CS307/BoilerCards/client/src/Components/Achievements.js",["168","169"],[],"import React, { useState } from \"react\";\nimport \"./Achievements.css\";\n\nfunction Achievements() {\n\n    return (\n        <div className=\"box-achievement\">\n            <h1 className=\"section-title\">Achievements</h1>\n            <div className= \"achievement-box\">\n                <button className= \"block-achievement\"><img className= \"img-achievement\" src= {require(\"../images/PurdueTrain.png\")} alt=\"my image\"/></button>\n            </div>\n        </div>\n    );\n}\n\nexport default Achievements;","/Users/jojo/Desktop/CS307/BoilerCards/client/src/Components/HomeClasses.js",["170"],[],"import React, { useState } from \"react\";\nimport \"./HomeClasses.css\";\n\nfunction HomeClasses() {\n\n    return (\n        <div className=\"box-classes\">\n            <h1 className=\"section-title\">My Classes</h1>\n            <div className= \"home-boxes\">\n                \n                <button className= \"block-buttons\"><img className= \"img-class\" src= {require(\"../images/PurdueTrain.png\")} alt=\"folder\"/></button>\n         